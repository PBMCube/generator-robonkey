{
  "projectURL": "<%= projectUrl %>",<% if(scopifyOption){ %>
  "scope": "#scope",<% } %><% if(classprefixOption){ %>
  "prefix": "prfx-",<% } %><% if(modernizrOption){ %>
  "modernizr": {
    "excludeTests": [],
    "tests": [],
    "output": "modernizr.custom.js",
    "options": [ "setClasses", "addTest", "html5printshiv", "testProp", "fnBind" ]
  },<% } %><% if(customIconfontOption) { %>"iconFont": {
    "name": "<%= customIconFontName %>",
    "types": ["ttf", "eot", "woff", "woff2", "svg"]
  }, <% } %>
  "foo": "bar",
  "src": "./../src/",
  "dest": "./..<%= templateDest %>",
  "resrc": {
    "main": "./..<%= assetsDirPath %>/",
    "js": "./..<%= templateDest %><%= jsDirPath %>/",
    "jsvendor": "./..<%= templateDest %><%= jsLibDirPath %>/",
    "img": "./..<%= templateDest %><%= imgDirPath %>/",
    "css": "./..<%= templateDest %><%= cssDirPath %>/",
    "cssvendor": "./..<%= templateDest %><%= cssLibDirPath %>/",
    "fonts": "./..<%= templateDest %><%= fontDirPath %>/",
    "jade": "./..<%= templateDest %>/"
  }
}
