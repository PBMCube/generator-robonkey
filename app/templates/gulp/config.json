{
  "projectURL": "<%= localUrl %>",<% if(includePcssScopify){ %>
  "scope": "#scope",<% } %><% if(includePcssClassPrefix){ %>
  "prefix": "prfx-",<% } %><% if(includeModernizr){ %>
  "modernizr": {
    "excludeTests": [],
    "tests": [],
    "output": "modernizr.custom.js",
    "options": [ "setClasses", "addTest", "html5printshiv", "testProp", "fnBind" ]
  },<% } %><% if(includeCustomIcnFont) { %>"iconFont": {
    "name": "<%= customIconFontName %>",
    "types": ["ttf", "eot", "woff", "woff2", "svg"]
  }, <% } %>
  "src": "./../src/",
  "dest": "./..<%= templateDest %>",
  "resrc": {
    "main": "./..<%= templateDest %><% if(!isExpress){ %>/assets<% } %>/",
    "js": "./..<%= templateDest %><% if(!isExpress){ %>/assets/js/<% } %><% if(isExpress){ %>/javascripts/<% } %>",
    "jsvendor": "./..<%= templateDest %><% if(!isExpress){ %>/assets/js/<% } %><% if(isExpress){ %>/javascripts/<% } %>libs/",
    "img": "./..<%= templateDest %><% if(!isExpress){ %>/assets<% } %>/images/",
    "css": "./..<%= templateDest %><% if(!isExpress){ %>/assets/css/<% } %><% if(isExpress){ %>/stylesheets/<% } %>",
    "cssvendor": "./..<%= templateDest %><% if(!isExpress){ %>/assets/css/<% } %><% if(isExpress){ %>/stylesheets/<% } %>libs/",
    "fonts": "./..<%= templateDest %><% if(!isExpress){ %>/assets<% } %>/fonts/",
    "jade": "./..<%= templateDest %>/"
  }
}
