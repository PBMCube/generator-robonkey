{
  "projectURL": "<%= projectUrl %>",<% if(scopifyOption){ %>
  "scope": "#scope",<% } %><% if(classprefixOption){ %>
  "prefix": "prfx-",<% } %><% if(modernizrOption){ %>
  "modernizr": {
    "excludeTests": [],
    "tests": [],
    "output": "modernizr.custom.js",
    "options": [ "setClasses", "addTest", "html5printshiv", "testProp", "fnBind" ]
  },<% } %><% if(customIconfontOption) { %>"iconFont": {
    "name": "<%= customIconFontName %>",
    "types": ["ttf", "eot", "woff", "woff2", "svg"]
  }, <% } %>
  "foo": "bar",
  "src": "./../src/",
  "dest": "./..<%= templateDest %>",
  "resrc": {
    "main": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets<% } %>/",
    "js": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets/js/<% } %><% if(environmentOption === 'express'){ %>/javascripts/<% } %>",
    "jsvendor": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets/js/<% } %><% if(environmentOption === 'express'){ %>/javascripts/<% } %>libs/",
    "img": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets<% } %>/images/",
    "css": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets/css/<% } %><% if(environmentOption === 'express'){ %>/stylesheets/<% } %>",
    "cssvendor": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets/css/<% } %><% if(environmentOption === 'express'){ %>/stylesheets/<% } %>libs/",
    "fonts": "./..<%= templateDest %><% if(environmentOption !== 'express'){ %>/assets<% } %>/fonts/",
    "jade": "./..<%= templateDest %>/"
  }
}
