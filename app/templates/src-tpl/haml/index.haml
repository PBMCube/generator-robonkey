!!! 5
%html.no-js.nl(lang="nl")
  %head
    %meta(charset="utf-8")
    %meta(content="IE=edge" http-equiv="X-UA-Compatible")
    %title pageTitle
    %meta(content="Description" name="description")
    %meta(name="viewport" content="width=device-width, initial-scale=1")
    %link(href="apple-touch-icon.png" rel="apple-touch-icon")
    / Place favicon.ico in the root directory
    / build:css
    %link(href="/assets/css/style.css" rel="stylesheet")
    / endbuild
    <% if(includeModernizr){ %>
    / build:modernizr
    %script(src="assets/js/libs/modernizr.dev.js")
    /endbuild <% } %>
  %body
    <% if(includeJQuery){ %>%script(src="assets/js/libs/jquery.min.js")<% } %>
    <% if(includeWaypoints && includeJQuery){ %>%script(src="assets/js/libs/jquery.waypoints.min.js")<% } else if(includeWaypoints) { %>%script(src="assets/js/libs/noframework.waypoints.min.js")<% } %><% if(includeSignals){ %>
    %script(src="assets/js/libs/signals.min.js")<% } %><% if(includeD3){ %>
    %script(src="assets/js/libs/d3.min.js")<% } %><% if(includeEnquire){ %>
    %script(src="assets/js/libs/enquire.min.js")<% } %><% if(includeTweenmax){ %>
    %script(src="assets/js/libs/TweenMax.min.js")<% } %>
    / build:js
    %script(sr="/assets/js/script.js")
    / endbuild

    <% if(includeGA){ %>
    %script
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        ga('create', 'UA-XXXXX-X', 'auto');
        ga('send', 'pageview');<% } %>
